# See documentation at docs/devel/tracing.rst

# riscv-iommu.c
riscv_iommu_new(int bus, int slot, int func, int dbus, int dslot, int dfunc) "NEW %04x:%02x.%d attached %04x:%02x.%d"
riscv_iommu_flt(int bus, int slot, int func, uint64_t reason, uint64_t iova) "FLT %04x:%02x.%d reason: 0x%"PRIx64" iova: 0x%"PRIx64
riscv_iommu_dma(int bus, int slot, int func, int pasid, const char *dir, uint64_t iova, uint64_t phys) "TLB %04x:%02x.%d #%d %s 0x%"PRIx64" -> 0x%"PRIx64
riscv_iommu_cmd(int bus, int slot, int func, uint64_t l, uint64_t u) "CMD %04x:%02x.%d 0x%"PRIx64" 0x%"PRIx64

